{
  "name": "og",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "stylelint": "stylelint src/**/*.css",
    "stylelint:fix": "stylelint --fix src/**/*.css",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint:fix",
      "yarn format"
    ],
    "src/**/*.css": [
      "yarn stylelint:fix",
      "yarn format"
    ]
  },
  "packageManager": "yarn@4.3.1",
  "volta": {
    "node": "20.15.1"
  },
  "dependencies": {
    "budoux": "^0.6.2",
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/node": "20.14.11",
    "@types/react": "18.3.3",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "husky": "9.0.11",
    "lint-staged": "15.2.7",
    "prettier": "3.3.3",
    "stylelint": "^16.1.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-prettier": "^5.0.0",
    "typescript": "5.5.3"
  }
}
